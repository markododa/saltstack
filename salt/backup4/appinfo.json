{
    "name": "backup4",
    "description": "Experimental backuppc version 4.",
    "version": "1.1",
    "icon": "fa-database",
    "dependency": "",
    "substates": ["backup"],
    "module": "va_backup4",
    "services": [{
        "name": "VAR_HOSTNAME",
        "checks": [{
            "id": "ssh",
            "name": "SSH Interface",
            "tcp": "VAR_DNS:22",
            "interval": "30s",
            "timeout": "5s"
        }, {
            "id": "web",
            "name": "Web Interface",
            "http": "http://VAR_DNS:80",
            "tls_skip_verify": true,
            "method": "POST",
            "interval": "30s",
            "timeout": "5s"
        }]

    }],
    "path": "-",
    "panels": {
        "admin": [{
            "key": "backup.hosts",
            "name": "Sources and backups"
        }, {
            "key": "backup.manage",
            "name": "Paths and filters"
        }, {
            "key": "backup.schedule",
            "name": "Schedule"
        }, {
            "key": "backup.overview",
            "name": "Overview"
        }],
        "user": []
    }
}
