server_host     = {{ dcip }}
server_port     = 389
version         = 3
bind            = yes
start_tls       = no
bind_dn         = {{ query_user }}
bind_pw         = {{ query_password }}
search_base     = {{ search_base }}
scope           = sub
query_filter    = (&(objectClass=group)(sAMAccountName=%u))
special_result_attribute = member
leaf_result_attribute = mail
result_attribute= userPrincipalName
debuglevel      = 0
